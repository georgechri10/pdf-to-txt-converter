{
  "functions": {
    "api/convert.py": {
      "maxDuration": 60
    }
  },
  "builds": [
    {
      "src": "api/convert.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/convert",
      "dest": "/api/convert.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
