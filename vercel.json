{
  "builds": [
    {
      "src": "api/convert.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/convert",
      "dest": "/api/convert.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
